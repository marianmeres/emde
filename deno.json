{
	"name": "@marianmeres/emde",
	"version": "1.0.8",
	"exports": {
		".": "./src/mod.ts",
		"./cli": "./src/cli.ts"
	},
	"tasks": {
		"test": "deno test -A --watch",
		"build:example": "deno run -A src/cli.ts --indir example/src --outdir example/dist --force --verbose"
	},
	"imports": {
		"@marianmeres/item-collection": "jsr:@marianmeres/item-collection@^1.2.19",
		"@std/assert": "jsr:@std/assert@^1.0.14",
		"@std/cli": "jsr:@std/cli@^1.0.21",
		"@std/collections": "jsr:@std/collections@^1.1.3",
		"@std/encoding": "jsr:@std/encoding@^1.0.10",
		"@std/fmt": "jsr:@std/fmt@^1.0.8",
		"@std/fs": "jsr:@std/fs@^1.0.19",
		"@std/path": "jsr:@std/path@^1.1.2",
		"@std/yaml": "jsr:@std/yaml@^1.0.9",
		"lodash": "npm:lodash@^4.17.21",
		"marked": "npm:marked@^15.0.12"
	},
	"fmt": {
		"useTabs": true,
		"lineWidth": 90,
		"indentWidth": 4
	},
	"lint": {
		"rules": {
			"exclude": ["no-explicit-any"]
		}
	}
}
